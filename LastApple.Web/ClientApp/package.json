{
    "name": "LastApple",
    "version": "0.1.0",
    "private": true,
    "homepage": "/",
    "dependencies": {
        "@aspnet/signalr": "^1.1.4",
        "@fortawesome/fontawesome-free": "^5.8.2",
        "@fortawesome/fontawesome-svg-core": "^1.2.18",
        "@fortawesome/free-brands-svg-icons": "^5.12.0",
        "@fortawesome/free-solid-svg-icons": "^5.8.2",
        "@fortawesome/react-fontawesome": "^0.1.4",
        "@types/musickit-js": "^1.0.5",
        "@types/react": "^16.9.32",
        "@types/react-bootstrap-typeahead": "^3.4.0",
        "@types/react-dom": "^16.0.11",
        "@types/react-router-dom": "^4.3.1",
        "bootstrap": "^4.3.1",
        "cordova-browser": "^6.0.0",
        "cordova-ios": "^6.1.1",
        "cordova-plugin-background-mode": "git+https://github.com/centogram/cordova-plugin-background-mode.git",
        "cordova-plugin-customurlscheme": "^4.4.0",
        "cordova-plugin-device": "^2.0.2",
        "cordova-plugin-safariviewcontroller": "1.6.0",
        "mobx": "^5.9.4",
        "mobx-react": "^5.4.4",
        "mobx-react-router": "^4.0.7",
        "nl.kingsquare.cordova.background-audio": "^1.0.1",
        "react": "^16.8.6",
        "react-bootstrap": "^1.4.3",
        "react-bootstrap-typeahead": "^4.0.0-alpha.10",
        "react-dom": "^16.8.6",
        "react-router-bootstrap": "^0.24.4",
        "react-router-dom": "^4.2.2",
        "react-scripts": "^3.4.4",
        "react-switch": "^5.0.0",
        "rimraf": "^2.6.3",
        "typescript": "^4.1.3"
    },
    "scripts": {
        "start": "rimraf ./www && rimraf ./build && react-scripts start",
        "build": "rimraf ./www && react-scripts build",
        "build:cordova": "rimraf ./www && react-scripts build && node prepareCordovaHtml.js && move build www",
        "build:cordova:mac": "rimraf ./www && react-scripts build && node prepareCordovaHtml.js && mv build www",
        "build:ios": "npm run build:cordova:mac && cordova prepare ios",
        "test": "react-scripts test --env=jsdom",
        "eject": "rimraf ./www && react-scripts eject"
    },
    "browserslist": [
        ">0.2%",
        "not dead",
        "not ie <= 11",
        "not op_mini all"
    ],
    "devDependencies": {
        "cordova-icon": "^1.0.0",
        "cordova-plugin-whitelist": "^1.3.3"
    },
    "cordova": {
        "plugins": {
            "cordova-plugin-whitelist": {},
            "nl.kingsquare.cordova.background-audio": {},
            "cordova-plugin-background-mode": {},
            "cordova-plugin-customurlscheme": {
                "URL_SCHEME": "lastapple"
            },
            "cordova-plugin-safariviewcontroller": {}
        },
        "platforms": [
            "browser",
            "ios"
        ]
    }
}
