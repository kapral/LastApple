{
    "name": "LastApple",
    "version": "0.1.0",
    "private": true,
    "homepage": "/",
    "devDependencies": {
        "@babel/plugin-proposal-private-property-in-object": "7.21.0",
        "@types/musickit-js": "^1.0.5",
        "@types/react": "18.2.14",
        "@types/react-dom": "18.2.6",
        "@types/react-router": "5.1.20",
        "@types/react-router-dom": "5.3.3",
        "@fortawesome/react-fontawesome": "0.2.0",
        "cordova-icon": "^1.0.0",
        "cordova-plugin-whitelist": "^1.3.3",
        "typescript": "4.9.5"
    },
    "dependencies": {
        "@aspnet/signalr": "^1.1.4",
        "@fortawesome/fontawesome-free": "6.4.0",
        "@fortawesome/fontawesome-svg-core": "6.4.0",
        "@fortawesome/free-brands-svg-icons": "6.4.0",
        "@fortawesome/free-solid-svg-icons": "6.4.0",
        "bootstrap": "5.3.0",
        "cordova-browser": "^6.0.0",
        "cordova-ios": "^6.1.1",
        "cordova-plugin-background-mode": "git+https://github.com/centogram/cordova-plugin-background-mode.git",
        "cordova-plugin-customurlscheme": "^4.4.0",
        "cordova-plugin-device": "^2.0.2",
        "cordova-plugin-safariviewcontroller": "1.6.0",
        "nl.kingsquare.cordova.background-audio": "^1.0.1",
        "react": "18.2.0",
        "react-bootstrap": "2.8.0",
        "react-bootstrap-typeahead": "6.2.3",
        "react-dom": "18.2.0",
        "react-router": "5.3.4",
        "react-router-bootstrap": "0.24.4",
        "react-router-dom": "5.3.4",
        "react-scripts": "5.0.1",
        "react-switch": "7.0.0",
        "rimraf": "^2.6.3"
    },
    "scripts": {
        "start": "rimraf ./www && rimraf ./build && react-scripts start",
        "build": "rimraf ./www && react-scripts build",
        "build:cordova": "rimraf ./www && react-scripts build && node prepareCordovaHtml.js && move build www",
        "build:cordova:mac": "rimraf ./www && react-scripts build && node prepareCordovaHtml.js && mv build www",
        "build:ios": "npm run build:cordova:mac && cordova prepare ios",
        "test": "react-scripts test --env=jsdom",
        "eject": "rimraf ./www && react-scripts eject"
    },
    "browserslist": [
        ">0.2%",
        "not dead",
        "not ie <= 11",
        "not op_mini all"
    ],
    "cordova": {
        "plugins": {
            "cordova-plugin-whitelist": {},
            "nl.kingsquare.cordova.background-audio": {},
            "cordova-plugin-background-mode": {},
            "cordova-plugin-customurlscheme": {
                "URL_SCHEME": "lastapple"
            },
            "cordova-plugin-safariviewcontroller": {}
        },
        "platforms": [
            "browser",
            "ios"
        ]
    }
}
